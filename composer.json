{
  "name": "UserAPI",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "slim/slim": "^3.9",
    "illuminate/database": "^5.4",
    "illuminate/events": "^5.4",
    "robmorgan/phinx":"^0.9",
    "respect/validation": "^1.1",
    "fzaninotto/faker": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "User\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": ["src/database/"],
    "psr-4": {
      "Tests\\": "src/Test/"
    }
  }
}